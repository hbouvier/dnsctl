{
  "author": "Henri Bouvier",
  "name": "dnsctl",
  "description": "A client for dns",
  "version": "0.2.0",
  "keywords": [
    "dns"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hbouvier/dnsctl.git"
  },
  "dependencies": {
    "node-options": "0.0.3",
    "winston": "0.7.2"
  },
  "engines": {
    "node": ">= 0.10.0 < 0.11.0"
  },
  "main": "./lib/dnsctl.js",
  "bin": {
    "dnsctl": "./bin/dnsctl"
  },
  "scripts": {
    "start": "bin/dnsctl"
  },
  "configuration": {
    "level": "error",
    "dns_urls": "http://127.0.0.1:8053",
    "context" : "/dns/api/v1",
    "dns": {
      "zone": "local.dev",
      "ttl": 3600,
      "prefix": "dns:",
      "timeout": 1000
    }
  }
}
